Options -ExecCGI -Indexes
<IfModule version.c>
    <IfVersion >= 2.4>
        Require all denied
    </IfVersion>
    <IfVersion < 2.4>
        Deny from all
    </IfVersion>
</IfModule>
<FilesMatch "\.(png)$">
    <IfModule version.c>
        <IfVersion >= 2.4>
            Require all granted
        </IfVersion>
        <IfVersion < 2.4>
            Allow from all
        </IfVersion>
    </IfModule>
</FilesMatch>